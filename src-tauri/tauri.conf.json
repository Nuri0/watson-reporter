{
  "$schema": "../node_modules/@tauri-apps/cli/schema.json",
  "build": {
    "beforeBuildCommand": "npm run build_desktop",
    "beforeDevCommand": "npm run start_desktop",
    "frontendDist": "../dist/desktop/browser",
